@model TFCLPortal.ApplicationWiseDeviationVariables.Dto.ApplicationWiseDeviationVariableListDto

@{
    ViewData["Title"] = "ApplicationWiseDetails";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

<=-- ### $App Screen Content ### -->
<main class='main-content bgc-grey-100' style="padding-top:50px">
    <div class="row page-title">
        <div class="col-md-6 text-left">
            <h4 class="page-title-text">
                Application Wise Deviation Matrix Details
            </h4>
            <h6 class="page-title-breadcrumbs">
                <a href="~/Dashboard/Dashboard">Home</a> > Deviation Matrix > Deviation Matrices List > Details
            </h6>
        </div>
        <div class="col-md-6 text-right">
            @*<button class="btn btn-sm btn-outline-success" onclick="$('#filterForm').toggle();"><i class="fa fa-filter"></i> Filter Records</button>*@
            <a href="~/Deviation/ApplicationWiseindex" class="btn btn-sm btn-outline-success bg1 eft-1" title="Back"><i class="fa fa-angle-double-left"></i> Back To Listing </a>
            <a asp-action="ApplicationWiseEdit" asp-route-id="@Model.ApplicationId" class="btn btn-sm btn-outline-primary bg1 eft-1" title="Edit"><i class="fa fa-edit"></i> Edit</a>
        </div>
    </div>
    <div class="card p-10 mB-20">
        <div>
            <div class="row">
                <div class="col-md-4">
                    <p class="detailHeading">
                        Client ID
                    </p>
                    <p class="detailText">
                        @(Model.ClientID==null || Model.ClientID == "" ? "--" : Model.ClientID)
                    </p>
                </div>
                <div class="col-md-4">
                    <p class="detailHeading">
                        APPLICANT / RESPONDENT NAME
                    </p>
                    <p class="detailText">
                        @(Model.ClientName==null || Model.ClientName == "" ? "--" : Model.ClientName)
                    </p>
                </div>
