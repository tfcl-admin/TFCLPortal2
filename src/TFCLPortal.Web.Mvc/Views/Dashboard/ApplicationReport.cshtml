                        <td class="title" width="25%">S/O D/O W/O</td>
                        <td class="text" width="20%">@(ViewBag.PD.ParentName == null || ViewBag.PD.ParentName == "" ? "--" : ViewBag.PD.ParentName)</td>
                    </tr>
                    <tr>

