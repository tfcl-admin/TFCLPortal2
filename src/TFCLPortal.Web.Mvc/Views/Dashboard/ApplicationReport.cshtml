                        <td class="title">ALTERNATE MOBILE # (Self)</td>
                        <td class="text">@(ViewBag.CD.AlternateMobileNoSelf == null || ViewBag.CD.AlternateMobileNoSelf == "" ? "--" : ViewBag.CD.AlternateMobileNoSelf)</td>
                    </tr>
                    <tr>

                        <td class="title">ALTERNATE MOBILE # (Family)</td>
                        <td class="text">@(ViewBag.CD.AlternateMobileNo == null || ViewBag.CD.AlternateMobileNo == "" ? "--" : ViewBag.CD.AlternateMobileNo)</td>

                        <td class="title">Relationship with Applicant</td>
                        <td class="text">@(ViewBag.CD.AlternateMobileNoName == null || ViewBag.CD.AlternateMobileNoName == "" ? "--" : ViewBag.CD.AlternateMobileNoName)</td>

                    </tr>
                </tbody>
            </table>

        }
        @if (ViewBag.BD != null)
        {



            <table border="1" width="100%" class="tbl">
                <tr>
                    <td class="title text-center bg-gray ScreenTitle" colspan="6">Business Details</td>
                </tr>
                <tr>
                    <td class="title" width="30%">Experience of Running School (Years)</td>
                    <td class="text" width="25%">@(ViewBag.BD.TotalExperienceInEducationIndustry == null || ViewBag.BD.TotalExperienceInEducationIndustry == "" ? "--" : ViewBag.BD.TotalExperienceInEducationIndustry)</td>

                    <td class="title" width="25%">Applicant's Previous Experience</td>
                    <td class="text" width="20%">@(ViewBag.BD.PreviousExperience == null || ViewBag.BD.PreviousExperience == "" ? "--" : ViewBag.BD.PreviousExperience)</td>

                </tr>
                <tr>
                    <td class="title">School NTN Number</td>
                    <td class="text">@(ViewBag.BD.SchoolNtn == null || ViewBag.BD.SchoolNtn == "" ? "--" : ViewBag.BD.SchoolNtn)</td>
                    <td class="title">Active Tax Payer (School)</td>
                    @if (ViewBag.BD.isActiveTaxPayer == null)
                    {
                        <td class="text">--</td>
                    }
                    else if (ViewBag.BD.isActiveTaxPayer == false)
                    {
                        <td class="text">No</td>
                    }
                    else if (ViewBag.BD.isActiveTaxPayer == true)
                    {
                        <td class="text">Yes</td>
                    }



                </tr>
                <tr>
                    <td class="title">Other Campuses/Branches/Institutions</td>
                    @if (ViewBag.BD.isOtherCampus == null)
                    {
                        <td class="text">--</td>
                    }
                    else if (ViewBag.BD.isOtherCampus == false)
                    {
                        <td class="text">No</td>
                    }
                    else if (ViewBag.BD.isOtherCampus == true)
                    {
                        <td class="text">Yes</td>
                    }

                    <td class="title">
                        No of Campus(es)
                    </td>
                    @foreach (var item in ViewBag.BD.school_Branches)
                    {
                        if (@item.ClientBusinessClassificationName == "Campus")
                        {
                            campusSum++;
                        }
                    }
                    <td class="text">@campusSum</td>

                </tr>

                <tr>
                    <td class="title">No of Branch(es)</td>
                    @foreach (var item in ViewBag.BD.school_Branches)
                    {
                        if (@item.ClientBusinessClassificationName == "Branch")
                        {
                            branchesSum++;
                        }
                    }
                    <td class="text">@branchesSum</td>
                    <td class="title">No of Campus(es)</td>
                    @foreach (var item in ViewBag.BD.school_Branches)
                    {
                        if (@item.ClientBusinessClassificationName == "Institute")
                        {
                            institutesSum++;
                        }
                    }
                    <td class="text">@institutesSum</td>
                </tr>

                @if (ViewBag.BD.school_Branches != null)
                {
                    foreach (var item in ViewBag.BD.school_Branches)
                    {
                        <tr>
                            <td class="title text-left bg-gray" colspan="4">@item.SchoolName</td>
                        </tr>
                        <tr>
                            <td class="title">Client/Business Name</td>
                            <td class="text">@(item.SchoolName == null || item.SchoolName == "" ? "--" : item.SchoolName)</td>

                            <td class="title">Client/Business Classification</td>
                            <td class="text">@(item.ClientBusinessClassificationName == null || item.ClientBusinessClassificationName == "" ? "--" : item.ClientBusinessClassificationName)</td>


                        </tr>

                        <tr>
                            <td class="title">
                                Address
                            </td>
                            <td class="text">@(item.SchoolAddress == null || item.SchoolAddress == "" ? "--" : item.SchoolAddress)</td>
                            <td class="title">
                                Mouza/Town
                            </td>
                            <td class="text">@(item.Moza_Town == null || item.Moza_Town == "" ? "--" : item.Moza_Town)</td>

                        </tr>
                        <tr>
                            <td class="title">
                                Union Council
                            </td>
                            <td class="text">@(item.UC_NO == null || item.UC_NO == "" ? "--" : item.UC_NO)</td>

                            <td class="title">
